,YAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,KADA;AAEA,kBAAA,IAAA,EAAA,MAAA,CAAA;AAFA,iBAAA,CAJA;;AAAA;AAIA,gBAAA,EAJA;AAQA,gBAAA,MAAA,CAAA,OAAA,GAAA,EAAA,CAAA,IAAA;AARA;AAAA;;AAAA;AAAA;AAAA;AAUA,gBAAA,OAAA,CAAA,GAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KAhBA;AAiBA,IAAA,QAjBA,sBAiBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,YAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAMA,gBAAA,OAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KAzBA;AA0BA,IAAA,UA1BA,sBA0BA,IA1BA,EA0BA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KA7BA;AA8BA,IAAA,UA9BA,sBA8BA,IA9BA,EA8BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAJA,kDAKA,IALA;;AAAA;AAAA;AAAA;AAOA,gBAAA,OAAA,CAAA,GAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAvCA;AAwCA,IAAA,QAxCA,oBAwCA,IAxCA,EAwCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA,KADA;AAEA,kBAAA,IAAA,EAAA,MAAA,CAAA,QAAA,CAAA;AAFA,iBAAA,CAFA;;AAAA;AAMA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAPA,kDAQA,IARA;;AAAA;AAAA;AAAA;AAUA,gBAAA,OAAA,CAAA,GAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA;AApDA;AAxBA,CAAA","sourcesContent":["<template>\n  <div class=\"admin\">\n    <div class=\"heading\">\n      <h1>Make an Entry</h1>\n    </div>\n    <div class=\"add\">\n      <div class=\"form\">\n        <div class=\"info\">\n          <input v-model=\"title\" placeholder=\"Title\" />\n          <textarea v-model=\"desc\" placeholder=\"Description\" />\n        </div>\n        <button @click=\"upload\">Upload</button>\n      </div>\n      <div class=\"upload\" v-if=\"addItem\">\n        <h2>{{ addItem.title }}</h2>\n        <p>{{ addItem.desc }}</p>\n      </div>\n    </div>\n    <div class=\"heading\">\n      <h1>Edit/Delete an Entry</h1>\n    </div>\n    <div class=\"edit\">\n      <div class=\"form\">\n        <input v-model=\"findTitle\" placeholder=\"Search\" />\n        <div class=\"suggestions\" v-if=\"suggestions.length > 0\">\n          <div\n            class=\"suggestion\"\n            v-for=\"s in suggestions\"\n            :key=\"s.id\"\n            @click=\"selectItem(s)\"\n          >\n            {{ s.title }}\n          </div>\n        </div>\n      </div>\n      <div class=\"upload\" v-if=\"findItem\">\n        <input v-model=\"findItem.title\" />\n        <textarea v-model=\"findItem.desc\"></textarea>\n      </div>\n      <div class=\"actions\" v-if=\"findItem\">\n        <button @click=\"deleteItem(findItem)\">Delete</button>\n        <button @click=\"editItem(findItem)\">Edit</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"Admin\",\n  data() {\n    return {\n      title: \"\",\n      desc: \"\",\n      file: null,\n      addItem: null,\n      items: [],\n      findTitle: \"\",\n      findItem: null,\n    };\n  },\n  computed: {\n    suggestions() {\n      let items = this.items.filter((item) =>\n        item.title.toLowerCase().startsWith(this.findTitle.toLowerCase())\n      );\n      return items.sort((a, b) => a.title > b.title);\n    },\n  },\n  created() {\n    this.getItems();\n  },\n  methods: {\n    fileChanged(event) {\n      this.file = event.target.files[0];\n    },\n    async upload() {\n      try {\n        const formData = new FormData();\n        formData.append(\"photo\", this.file);\n        let r2 = await axios.post(\"/api/items\", {\n          title: this.title,\n          desc: this.desc,\n        });\n        this.addItem = r2.data;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async getItems() {\n      try {\n        let response = await axios.get(\"/api/items\");\n        this.items = response.data;\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    selectItem(item) {\n      this.findTitle = \"\";\n      this.findItem = item;\n    },\n    async deleteItem(item) {\n      try {\n        await axios.delete(\"/api/items/\" + item._id);\n        this.findItem = null;\n        this.getItems();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async editItem(item) {\n      try {\n        await axios.put(\"/api/items/\" + item._id, {\n          title: this.findItem.title,\n          desc: this.findItem.desc,\n        });\n        this.findItem = null;\n        this.getItems();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\n.image h2 {\n  font-style: italic;\n  font-size: 1em;\n}\n\n.heading {\n  display: flex;\n  margin-bottom: 20px;\n  margin-top: 20px;\n}\n\n.heading h2 {\n  margin-top: 8px;\n  margin-left: 10px;\n}\n\n.add,\n.edit {\n  display: flex;\n}\n\n.circle {\n  border-radius: 50%;\n  width: 18px;\n  height: 18px;\n  padding: 8px;\n  background: #333;\n  color: #fff;\n  text-align: center;\n}\n\n/* Form */\ninput,\ntextarea,\nselect,\nbutton {\n  font-family: \"Montserrat\", sans-serif;\n  font-size: 1em;\n}\n\n.form {\n  margin-right: 50px;\n}\n\n.info {\n  display: flex;\n  flex-direction: column;\n  width: 20em;\n}\n\n.upload {\n  display: flex;\n  flex-direction: column;\n}\n\n/* Uploaded images */\n.upload h2 {\n  margin: 0px;\n}\n\n.upload img {\n  max-width: 300px;\n}\n\n/* Suggestions */\n.suggestions {\n  width: 200px;\n  border: 1px solid #ccc;\n}\n\n.suggestion {\n  min-height: 20px;\n}\n\n.suggestion:hover {\n  background-color: #5bdeff;\n  color: #fff;\n}\n</style>\n\n\n"],"sourceRoot":"src/views"}]}